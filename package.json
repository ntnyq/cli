{
  "name": "ntnyq",
  "type": "module",
  "version": "0.4.0",
  "packageManager": "pnpm@10.29.3",
  "description": "The ntnyq CLI",
  "keywords": [
    "cli",
    "ntnyq"
  ],
  "license": "MIT",
  "author": "ntnyq <ntnyq@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ntnyq/cli.git"
  },
  "exports": "./dist/cli.mjs",
  "main": "./dist/cli.mjs",
  "bin": "./dist/cli.mjs",
  "files": [
    "dist"
  ],
  "scripts": {
