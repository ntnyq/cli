{
  "name": "ntnyq",
  "version": "0.0.6",
  "description": "The ntnyq CLI",
  "keywords": [
    "ntnyq",
    "cli"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ntnyq/ntnyq.git"
  },
  "license": "MIT",
  "author": "ntnyq <ntnyq@gmail.com>",
  "main": "dist/cli.js",
  "exports": "./dist/cli.js",
  "bin": {
