  "dependencies": {
    "ink": "^6.7.0",
    "ink-big-text": "^2.0.0",
    "ink-gradient": "^4.0.0",
    "ink-link": "^5.0.0",
    "ink-select-input": "^6.2.0",
    "ink-text-input": "^6.0.0",
    "open": "^11.0.0",
    "react": "^19.2.4"
  },
  "devDependencies": {
    "@ntnyq/eslint-config": "^5.9.0",
    "@ntnyq/prettier-config": "^3.0.1",
    "@types/node": "^24.10.13",
    "@types/react": "^19.2.14",
    "bumpp": "^10.4.1",
    "eslint": "^9.39.2",
    "husky": "^9.1.7",
    "ink-testing-library": "^4.0.0",
    "nano-staged": "^0.9.0",
    "prettier": "^3.8.1",
    "tsdown": "^0.20.3",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  },
  "engines": {
    "node": ">=20.19.0"
  },
  "nano-staged": {
    "*.{js,ts,cjs,mjs,tsx,md,yml,yaml,json}": "eslint --fix"
  }
