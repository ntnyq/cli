    "open": "^8.4.0",
    "react": "^17.0.1"
  },
  "devDependencies": {
    "@ava/typescript": "^3.0.1",
    "@ntnyq/prettier-config": "^0.3.1",
    "@types/node": "^17.0.21",
    "@types/react": "^17.0.40",
    "ava": "^4.1.0",
    "ink-testing-library": "^2.1.0",
    "prettier": "^2.5.1",
    "typescript": "^4.6.2"
